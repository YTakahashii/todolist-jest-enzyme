    "@types/react-test-renderer": "^16.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^16.7.0",
    "react-testing-library": "^5.4.2",
    "typesafe-actions": "^2.0.4",
    "typescript": "^3.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
